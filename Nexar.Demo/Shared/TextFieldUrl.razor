
<MudField Label=@Label Variant=Variant.Outlined>
    <MudLink Href=@Href Target=_blank>@(Text ?? Href)</MudLink>
</MudField>

@code {
    [Parameter]
    public string? Label { get; init; }

    [Parameter]
    public string? Href { get; init; }

    [Parameter]
    public string? Text { get; init; }
}
